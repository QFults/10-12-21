{
  "name": "10-12-21",
  "version": "1.0.0",
  "description": "- Intro to GQL\r - Reducers and Global State\r - Homebrewed User Authentication",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*' \" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QFults/10-12-21.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/QFults/10-12-21/issues"
  },
  "homepage": "https://github.com/QFults/10-12-21#readme",
  "dependencies": {
    "apollo-server-express": "^3.4.0",
    "bcrypt": "^5.0.1",
    "concurrently": "^6.3.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.6.1",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.10"
  }
}